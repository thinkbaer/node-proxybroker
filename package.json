{
  "name": "proxybroker",
  "version": "0.11.0",
  "description": "Broker for anonymus proxies",
  "main": "dist/main.js",
  "scripts": {
    "test": "node_modules/.bin/nyc mocha --opts test/mocha.all.opts",
    "posttest": "node_modules/.bin/nyc report --reporter=json && codecov -f coverage/*.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinkbaer/node-proxybroker.git"
  },
  "keywords": [
    "proxy",
    "broker"
  ],
  "author": "Robert Kania",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thinkbaer/node-proxybroker/issues"
  },
  "homepage": "https://github.com/thinkbaer/node-proxybroker#readme",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.2.0",
    "@types/express": "^4.17.0",
    "@types/form-data": "^2.5.0",
    "@types/got": "^9.6.6",
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^5.2.7",
    "@types/multer": "^1.3.8",
    "@types/node": "^12.7.1",
    "@types/serve-static": "^1.13.2",
    "@types/tough-cookie": "^2.3.5",
    "@types/vinyl-fs": "^2.4.11",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "codelyzer": "^5.1.0",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.1.3",
    "gulp-clean": "^0.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-shell": "^0.7.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "merge-stream": "^2.0.0",
    "mocha": "^6.2.0",
    "mocha-typescript": "^1.1.17",
    "nyc": "^14.1.1",
    "sqlite3": "^4.0.9",
    "std-mocks": "^1.0.1",
    "ts-node": "^6.2.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "typescript-require": "^0.2.10",
    "pg": "^7.12.1",
    "pg-native": "^3.0.0"
  },
  "dependencies": {
    "@typexs/base": "^0.9.43",
    "@typexs/server": "^0.4.15",
    "commons-config": "^0.4.19",
    "commons-eventbus": "^0.11.2",
    "commons-http": "^1.0.7",
    "cron-parser": "^2.13.0",
    "form-data": "^2.5.0",
    "got": "^9.6.0",
    "multer": "^1.4.2",
    "proxy-agent": "^3.1.0",
    "tough-cookie": "^3.0.1"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false
  },
  "typexs": {
    "name": "proxybroker",
    "declareLibs": [
      {
        "topic": "proxy-providers",
        "refs": [
          "providers/proxy",
          "src/providers/proxy"
        ]
      }
    ]
  }
}
