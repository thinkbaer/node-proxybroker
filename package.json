{
  "name": "proxybroker",
  "version": "0.10.0",
  "description": "Broker for anonymus proxies",
  "main": "dist/main.js",
  "scripts": {
    "test": "node_modules/.bin/nyc mocha --opts test/mocha.all.opts",
    "posttest": "node_modules/.bin/nyc report --reporter=json && codecov -f coverage/*.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinkbaer/node-proxybroker.git"
  },
  "keywords": [
    "proxy",
    "broker"
  ],
  "author": "Robert Kania",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thinkbaer/node-proxybroker/issues"
  },
  "homepage": "https://github.com/thinkbaer/node-proxybroker#readme",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/form-data": "^2.2.1",
    "@types/got": "^9.4.4",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/multer": "^1.3.7",
    "@types/node": "^11.13.9",
    "@types/serve-static": "^1.13.2",
    "@types/tough-cookie": "^2.3.5",
    "@types/vinyl-fs": "^2.4.11",
    "chai": "^4.2.0",
    "codecov": "^3.3.0",
    "del": "^4.1.1",
    "gulp": "^4.0.1",
    "gulp-bump": "^3.1.3",
    "gulp-clean": "^0.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-shell": "^0.7.0",
    "gulp-typescript": "^5.0.1",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.1",
    "gulpclass": "^0.2.0",
    "mocha": "^6.1.4",
    "mocha-typescript": "^1.1.17",
    "nyc": "^14.1.0",
    "sqlite3": "^4.0.6",
    "std-mocks": "^1.0.1",
    "ts-node": "^6.2.0",
    "typescript": "^3.4.5",
    "typescript-require": "^0.2.10",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "@typexs/base": "^0.9.0",
    "@typexs/server": "^0.4.4",
    "commons-config": "^0.4.19",
    "commons-eventbus": "^0.11.0",
    "commons-http": "^1.0.1",
    "cron-parser": "^2.11.0",
    "form-data": "^2.3.3",
    "got": "^9.6.0",
    "multer": "^1.4.1",
    "tough-cookie": "^3.0.1"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false
  },
  "typexs": {
    "name": "proxybroker",
    "libs": {
      "topic": "proxy-providers",
      "refs": [
        "providers/proxy",
        "src/providers/proxy"
      ]
    }
  }
}
