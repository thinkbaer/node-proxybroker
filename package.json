{
  "name": "proxybroker",
  "version": "0.9.0",
  "description": "Broker for anonymus proxies",
  "main": "dist/main.js",
  "scripts": {
    "test": "nyc mocha --opts test/mocha.all.opts",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinkbaer/node-proxybroker.git"
  },
  "keywords": [
    "proxy",
    "broker"
  ],
  "author": "Robert Kania",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thinkbaer/node-proxybroker/issues"
  },
  "homepage": "https://github.com/thinkbaer/node-proxybroker#readme",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.12.18",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/serve-static": "^1.13.2",
    "chai": "^4.1.0",
    "codecov": "^2.1.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.7.0",
    "gulp-clean": "^0.3.2",
    "gulp-replace": "^0.6.1",
    "gulp-shell": "^0.6.3",
    "gulp-typescript": "^3.1.7",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.10",
    "gulpclass": "^0.1.2",
    "mocha": "^3.3.0",
    "mocha-typescript": "^1.1.2",
    "source-map-support": "^0.4.15",
    "std-mocks": "^1.0.1",
    "ts-node": "^6.2.0",
    "typescript": "^3.4.1",
    "typescript-require": "^0.2.10",
    "watchify": "^3.7.0",
    "sqlite3": "^4.0.6"
  },
  "dependencies": {
    "commons-config": "^0.4.19",
    "commons-eventbus": "^0.10.4",
    "@typexs/base": "^0.8.8",
    "@typexs/server": "^0.4.0",
    "cron-parser": "^2.10.0",
    "glob": "^7.1.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "multer": "^1.4.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false
  },
  "typexs": {
    "name": "proxybroker",
    "libs": {
      "topic": "proxy-providers",
      "refs": [
        "providers/proxy",
        "src/providers/proxy"
      ]
    }
  }
}
